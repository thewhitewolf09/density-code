{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:react/recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react", "unused-imports"],
	"rules": {
		"react/prop-types": 0,
		"no-unused-vars": "off",
		"unused-imports/no-unused-imports": "error",
		"linebreak-style": 0,
		"unused-imports/no-unused-vars": [
			"warn",
			{
				"vars": "all",
				"varsIgnorePattern": "^_",
				"args": "after-used",
				"argsIgnorePattern": "^_"
			}
		],
		"sort-imports": [
			"warn",
			{
				"ignoreDeclarationSort": true
			}
		]
	}
}
